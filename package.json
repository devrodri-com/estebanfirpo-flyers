    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "playwright": "^1.55.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
